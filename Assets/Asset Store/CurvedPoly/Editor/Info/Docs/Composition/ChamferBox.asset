    mod: 0
  - icons: []
    texts:
    - <b>X (Sides):</b> number of internal separations on X direction.
    mod: 0
  - icons: []
    texts:
    - <b>Y (Sides):</b> number of internal separations on Y direction.
    mod: 0
  - icons: []
    texts:
    - <b>Z (Sides):</b> number of internal separations on Z direction.
    mod: 0
  - icons: []
    texts:
    - <b>Main (Tessellation):</b> the hint assigned to the circles edges.
    mod: 0
  - icons: []
    texts:
    - <b>Bevel (Tessellation):</b> the hint assigned to all the edges of bevels.
    mod: 0
  - icons: []
    texts:
    - <b>Top</b> when disable, the top face of the box is not built.
    mod: 0
  - icons: []
    texts:
    - <b>Bottom</b> when disable, the bottom face of the box is not built.
    mod: 0
  - icons: []
    texts:
